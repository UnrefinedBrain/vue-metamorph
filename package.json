{
  "name": "vue-metamorph",
  "version": "0.0.2",
  "description": "Codemod framework for Vue files",
  "keywords": [
    "codemod",
    "vue",
    "ast"
  ],
  "bin": {
    "vue-metamorph": "scripts/scaffold.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UnrefinedBrain/vue-metamorph.git"
  },
  "author": {
    "name": "UnrefinedBrain",
    "email": "github@unrefinedbrain.dev",
    "url": "https://github.com/UnrefinedBrain"
  },
  "license": "MIT",
  "files": [
    "dist",
    "script/scaffold.cjs",
    "template"
  ],
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/vue-metamorph.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs",
      "types": "./dist/vue-metamorph.d.ts"
    }
  },
  "scripts": {
    "build": "scripts/build",
    "lint": "eslint src docs/.vitepress --ext .ts",
    "test": "vitest",
    "docs:dev": "pnpm build && vitepress dev docs",
    "docs:build": "pnpm build && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.23.23",
    "@microsoft/api-extractor": "^7.40.1",
    "@types/cli-progress": "^3.11.5",
    "@types/deep-diff": "^1.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitepress": "1.0.0-rc.41",
    "vitepress-plugin-twoslash": "^0.10.2",
    "vitest": "^1.2.2",
    "vue-metamorph": "link:."
  },
  "dependencies": {
    "@babel/parser": "^7.23.9",
    "ast-types": "^0.14.2",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "deep-diff": "^1.0.2",
    "diff": "^5.2.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "lodash-es": "^4.17.21",
    "magic-string": "^0.30.7",
    "recast": "^0.23.4",
    "table": "^6.8.1",
    "vue-eslint-parser": "^9.4.2"
  }
}
